{
  "kind": "collectionType",
  "collectionName": "businesses",
  "info": {
    "singularName": "business",
    "pluralName": "businesses",
    "displayName": "business",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "minLength": 3,
      "maxLength": 25,
      "unique": true
    },
    "address": {
      "type": "string",
      "required": true,
      "minLength": 2
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "businesses"
    },
    "BusinessPic": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "services": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::service.service",
      "mappedBy": "businesses"
    },
    "telephone": {
      "type": "string",
      "required": true
    },
    "galleryPhotos": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "bookings": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::booking.booking",
      "mappedBy": "businesses"
    },
    "email": {
      "type": "email"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "business"
    },
    "favourite_lists": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::favourite-list.favourite-list",
      "inversedBy": "businesses"
    },
    "openhour": {
      "type": "integer",
      "required": false,
      "max": 24,
      "min": 0
    },
    "closehour": {
      "type": "integer",
      "max": 24,
      "min": 0
    },
    "ratings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::rating.rating",
      "mappedBy": "business"
    }
  }
}
